<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Adding plane</title>
</head>
<body>
    <div>
        <h2>Adding plane</h2>
    </div>
    <form method="post">
        <label>ID:
            <input type="text" name="id"><br />
        </label>
        <label>Company:
            <input type="text" name="company"><br />
        </label>
        <label>Monday:
            <input type="text" name="mon"><br />
        </label>
        <label>Tuesday:
            <input type="text" name="tue"><br />
        </label>
        <label>Wednesday:
            <input type="text" name="wed"><br />
        </label>
        <label>Thursday:
            <input type="text" name="thu"><br />
        </label>
        <label>Friday:
            <input type="text" name="fri"><br />
        </label>
        <label>Saturday:
            <input type="text" name="sat"><br />
        </label>
        <label>Sunday:
            <input type="text" name="sun"><br />
        </label>
        <label>Departure:
            <input type="text" name="departure"><br />
        </label>
        <label>Arrival:
            <input type="text" name="arrival"><br />
        </label>
        <button type="submit">Submit</button>
    </form>
</body>
</html>
